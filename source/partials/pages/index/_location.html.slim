div[class="row mx-0 location-wrapper"]
  div[class="col-12"]
    div[class="row name"]
      = data.settings.locations.description
    div[class="row locations"]
      ul
        - data.settings.locations.cities.sort_by { |k| k["id"] }.each_with_index do |place, index|
          li
            = place.city
            |,
            =>
            = place.state
